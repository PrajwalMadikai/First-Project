    <script>
        function editProductOffer(id, title, discount, expireAt) {
            console.log("editProductOffer called with:", id);
    
            document.getElementById('Editname').value = title.trim(); // Ensure no extra spaces
            document.getElementById('Editdiscount').value = discount;
    
            const date = new Date(expireAt);
            const formattedDate = date.toISOString().split('T')[0];
            document.getElementById('Editdate').value = formattedDate;
    
            document.getElementById('editproductId').value = id;
    
            document.getElementById('editOfferFormContainer').style.display = 'block';
        }
    
        document.getElementById('cancelButton').addEventListener('click', function() {
            document.getElementById('editOfferFormContainer').style.display = 'none';
        });
    
    function editOffer() {

    // Collect form data
    let productId = document.getElementById('editproductId').value;
    let name = document.getElementById('Editname').value;
    let discount = document.getElementById('Editdiscount').value;
    let date = document.getElementById('Editdate').value;

    // Send data to the server
    fetch(`/admin/update-product-offer/${productId}`, {
        method: 'POST',
        headers: {
            'Content-Type': 'application/json'
        },
        body: JSON.stringify({productId,name,discount,date})
    })
    .then(response => response.json())
    .then(data => {
        if (data.success) {
            Swal.fire({
                title: 'Success!',
                text: 'Product offer updated successfully.',
                icon: 'success',
                confirmButtonText: 'OK'
            }).then(() => {
                document.getElementById('editOfferFormContainer').style.display = 'none';
                window.location.reload()
            });
        } else {
            Swal.fire({
                title: 'Error!',
                text: 'Failed to update the product offer.',
                icon: 'error',
                confirmButtonText: 'Try Again'
            });
        }
    })
    .catch(error => {
        console.error('Error:', error);
    });
}

    </script>
